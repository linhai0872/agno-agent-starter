# Agno Agent Service 环境变量


# Model API Keys
# 至少配置一个，推荐 OPENROUTER（支持 100+ 模型）

OPENROUTER_API_KEY=your_openrouter_api_key_here  # 必填（推荐）
OPENAI_API_KEY=
GOOGLE_API_KEY=
ANTHROPIC_API_KEY=
DASHSCOPE_API_KEY=
ARK_API_KEY=
OLLAMA_HOST=http://localhost:11434
LITELLM_API_KEY=
LITELLM_API_BASE=http://localhost:4000

# 工具 API Keys (可选)
# TAVILY_API_KEY=              # Deep Research Team 增强搜索（可选，默认使用 DuckDuckGo）


# Project API Keys (可选，用于 Team/Workflow 用量隔离)

# CONTENT_PIPELINE_KEY=sk-xxx
# RESEARCH_TEAM_KEY=sk-xxx


# Database

DATABASE_URL=postgresql+psycopg://ai:ai@localhost:5532/ai
POSTGRES_USER=ai
POSTGRES_PASSWORD=ai
POSTGRES_DB=ai


# Service

API_PORT=7777
API_HOST=0.0.0.0
DEBUG_MODE=true      # 开发模式开启热重载，生产环境改为 false
WORKERS=1            # 开发模式用 1，生产环境改为 4


# Security (可选)

# OS_SECURITY_KEY=your_security_key_here


# Tracing (可选)

ENABLE_TRACING=false
# TRACING_DB_URL=postgresql+psycopg://ai:ai@localhost:5532/ai
TRACING_BATCH_SIZE=256


# MCP (可选)

ENABLE_MCP_SERVER=false

# MCP DevTools (可选，用于 AI Agent 测试和调试)
# 零配置设计：自动从 API_PORT 和 DATABASE_URL 派生，无需手动配置
# 如需覆盖，取消下面的注释：
# MCP_DEVTOOLS_API_URL=http://127.0.0.1:7777
# MCP_DEVTOOLS_DB_URL=postgresql+psycopg://ai:ai@localhost:5532/ai
